{
  "name": "chlorine",
  "main": "./lib/main",
  "version": "0.7.3",
  "description": "Socket REPL client for Clojure and ClojureScript",
  "keywords": [
    "clojure",
    "clojurescript",
    "clojureclr",
    "clojerl",
    "babashka",
    "lumo",
    "plank",
    "joker",
    "repl",
    "interactive",
    "socket repl",
    "nrepl"
  ],
  "repository": "https://github.com/mauricioszabo/atom-chlorine",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "chlorine:connect-socket-repl"
    ]
  },
  "providedServices": {
    "everything.search": {
      "versions": {
        "0.1.0": "everything_provider"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "autocomplete_provider"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "status_bar_consumer"
      }
    }
  },
  "devDependencies": {
    "ansi_up": "^4.0.4",
    "create-react-class": "^15.6.3",
    "grim": "^2.0.2",
    "react": "^16.3.0",
    "react-dom": "^16.13.0",
    "shadow-cljs": "^2.9.10",
    "source-map-support": "^0.5.13",
    "ws": "^7.1.2"
  }
}
